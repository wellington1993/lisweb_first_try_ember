Contato: {{inputs/generic-input
            value=model.contato}}
E-mail: {{inputs/email-input
          id="txt-email"
          value=model.email
          vazioValido=true
          actionOnFimValidacao="actValidarEmail"}}
Fone: {{inputs/mask-telefone
        id="txt-fone"
        value=telefone
        vazioValido=true
        actionOnFimValidacao="actValidarTelefone"}}
Celular: {{inputs/mask-telefone
           id="txt-celular"
           value=celular
           vazioValido=true
           actionOnFimValidacao="actValidarCelular"}}
Fax: {{inputs/mask-telefone
       id="txt-fax"
       value=fax
       vazioValido=true
       actionOnFimValidacao="actValidarFax"}}
