<div class="container animated fadeIn">

  <div class="row">

    <div class="col-lg-3 col-md-6">
      {{dashboard-button
       icon="fa-bookmark-o"
       titulo="Categorias de Produto"
       actionOnClick="actRedirecionarDashboard"
       refParam="categorias-de-produto"}}
    </div>

    <div class="col-lg-3 col-md-6">
      {{dashboard-button
       icon="fa-tags"
       titulo="Marcas"
       actionOnClick="actRedirecionarDashboard"
       refParam="marcas"}}
    </div>

     <div class="col-lg-3 col-md-6">
       {{dashboard-button
         icon="fa-users"
         titulo="Pessoas"
         actionOnClick="actRedirecionarDashboard"
         refParam="pessoas"}}
     </div>

     <div class="col-lg-3 col-md-6">
       {{dashboard-button
         icon="fa-user"
         titulo="Tipos de Pessoas"
         actionOnClick="actRedirecionarDashboard"
         refParam="tipos-de-pessoa"}}
     </div>

  </div>

  <div class="row">

     <div class="col-lg-3 col-md-6">
       {{dashboard-button
         icon="fa-flask"
         titulo="Tipos de Produto"
         actionOnClick="actRedirecionarDashboard"
         refParam="tipos-de-produto"}}
     </div>

     <div class="col-lg-3 col-md-6">
       {{dashboard-button
         icon="fa-exchange"
         titulo="Unidades de Medida"
         actionOnClick="actRedirecionarDashboard"
         refParam="unidades-de-medida"}}
     </div>

     <div class="col-lg-3 col-md-6">
       {{dashboard-button
         icon="fa-file-text"
         titulo="Par√¢metros Nfse"
         actionOnClick="actRedirecionarDashboard"
