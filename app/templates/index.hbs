<div class="row animated fadeInUp">
   <div class="col-lg-3 col-md-6">
     {{dashboard-button
       icon="fa-users"
       titulo="Pessoas"
       actionOnClick="actRedirecionarDashboard"
       refParam="pessoas"}}
   </div>
   <div class="col-lg-3 col-md-6">
     {{dashboard-button
       icon="fa-user"
       titulo="Tipos de Pessoas"
       actionOnClick="actRedirecionarDashboard"
       refParam="tipos-de-pessoa"}}
   </div>
   <div class="col-lg-3 col-md-6">
     {{dashboard-button
      icon="fa-tags"
      titulo="Marcas"
